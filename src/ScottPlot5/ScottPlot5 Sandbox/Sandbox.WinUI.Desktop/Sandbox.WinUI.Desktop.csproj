    <ItemGroup>
        <PackageReference Include="Microsoft.WindowsAppSDK" Version="1.5.240802000" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\ScottPlot5 Controls\ScottPlot.WinUI\ScottPlot.WinUI.csproj" />
    </ItemGroup>

