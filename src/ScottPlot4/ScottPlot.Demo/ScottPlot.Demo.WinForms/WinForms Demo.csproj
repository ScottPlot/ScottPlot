<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <UseWindowsForms>true</UseWindowsForms>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <RootNamespace>ScottPlot.Demo.WinForms</RootNamespace>
        <AssemblyName>ScottPlot.Demo.WinForms</AssemblyName>
        <StartupObject>ScottPlot.Demo.WinForms.Program</StartupObject>
        <PackageIcon></PackageIcon>
        <ApplicationIcon>icon.ico</ApplicationIcon>
        <LangVersion>11</LangVersion>
        <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\ScottPlot\ScottPlot.csproj" />
        <ProjectReference Include="..\..\ScottPlot.Cookbook\ScottPlot.Cookbook.csproj" />
        <ProjectReference Include="..\..\ScottPlot.WinForms\ScottPlot.WinForms.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="System.Drawing.Common" Version="4.7.2" />
        <PackageReference Include="System.Memory" Version="4.5.5" />
        <PackageReference Include="System.Resources.Extensions" Version="7.0.0" />
        <PackageReference Include="Microsoft.CodeAnalysis.Metrics" Version="*" >
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
    </ItemGroup>

</Project>