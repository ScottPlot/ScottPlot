<Window x:Class="ScottPlot.Demo.WPF.WpfDemosWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScottPlot.Demo.WPF"
        mc:Ignorable="d"
        Background="WhiteSmoke"
        Title="ScottPlot WPF Demos" Height="300" Width="400">
    <DockPanel>
        <WrapPanel>
            <Button Padding="5" Margin="5" Content="Mouse Tracker" Click="LaunchMouseTracker"/>
            <Button Padding="5" Margin="5" Content="Linked Axes" IsEnabled="False"/>
            <Button Padding="5" Margin="5" Content="Procedural Plot Generation" IsEnabled="False"/>
        </WrapPanel>
    </DockPanel>
</Window>
