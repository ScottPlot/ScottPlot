<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.0;net45</TargetFrameworks>
		<AssemblyName>ScottPlot</AssemblyName>
		<RootNamespace>ScottPlot</RootNamespace>
		<UseWindowsForms>true</UseWindowsForms>
		<UseWPF>true</UseWPF>
		<AssemblyTitle>ScottPlot</AssemblyTitle>
		<AssemblyProduct>ScottPlot</AssemblyProduct>
		<AssemblyVersion>3.1.0.0</AssemblyVersion>
		<AssemblyFileVersion>3.1.0.0</AssemblyFileVersion>
		<Company>Harden Technologies, LLC</Company>
		<Authors>Scott Harden</Authors>
		<Version>3.1.0</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Copyright />
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Description>ScottPlot is a free and open-source interactive graphing library for .NET</Description>
		<PackageTags>plotting plot data graph chart signal C# data-visualization winforms dotnetcore netcore</PackageTags>
		<PackageProjectUrl>https://github.com/swharden/ScottPlot</PackageProjectUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/swharden/ScottPlot/master/dev/icon/icon.png</PackageIconUrl>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net45|AnyCPU'">
	  <DefineConstants>DEBUG;TRACE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<None Update="ScottPlotWPF.xaml">
			<Generator>MSBuild:Compile</Generator>
		</None>
	</ItemGroup>

</Project>